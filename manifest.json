{
  "manifest_version": 3,
  "name": "HubSpot CRM Data Extractor",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["https://app-na2.hubspot.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "ui/dist/index.html"
  }
}
